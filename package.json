{
  "name": "llm-gateway",
  "version": "1.0.0",
  "description": "基于 Portkey Gateway 的轻量级 LLM 网关管理系统",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build:agent:optional && tsx watch src/index.ts",
    "build": "tsc",
    "build:agent": "node scripts/build-agent.js",
    "build:agent:optional": "node scripts/build-agent.js || true",
    "start": "npm run build:agent:optional && node dist/index.js",
    "start:all": "npm run build:agent:optional && node start.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "gateway",
    "portkey",
    "web-ui",
    "openai",
    "api-gateway",
    "load-balancing",
    "failover"
  ],
  "author": "SXueckShen <sxueck@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sxueck/llm-gateway.git"
  },
  "bugs": {
    "url": "https://github.com/sxueck/llm-gateway/issues"
  },
  "homepage": "https://github.com/sxueck/llm-gateway#readme",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/static": "^8.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "nanoid": "^5.0.9",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sql.js": "^1.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/sql.js": "^1.4.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
